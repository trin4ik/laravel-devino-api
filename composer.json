{
    "name": "trin4ik/laravel-devino-api",
    "description": "Send SMS by Devino.API (https://devinotele.com) from Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "trin4ik",
            "email": "trin4ik@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "guzzlehttp/guzzle": "^6.3"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Trin4ik\\DevinoApi\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trin4ik\\DevinoApi\\DevinoApiServiceProvider"
            ],
            "aliases": {
                "SMS": "Trin4ik\\DevinoApi\\Facades\\DevinoApi"
            }
        }
    }
}
